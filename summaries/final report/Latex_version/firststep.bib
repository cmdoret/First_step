issn = {1465-6914},
journal = {Genome Biology},
number = {11},
pages = {R131},
pmid = {24289259},
title = {{Chromatin signatures at transcriptional start sites separate two equally populated yet distinct classes of intergenic long noncoding RNAs.}},
url = {http://genomebiology.com/2013/14/11/R131$\backslash$npapers3://publication/doi/10.1186/gb-2013-14-11-r131},
